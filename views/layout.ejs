  <body>
    <%- include('_left_navigation'); %>

    <div id="main-body"><%- body %></div>

    <div class="css-blurry-gradient1"></div>

    <div class="css-blurry-gradient2"></div>

    <div class="css-blurry-gradient3"></div>

    <div class="css-blurry-gradient4"></div>

    <%- script %>
    <script src="<%= assetPath('js/background_color_animation.js') %>"></script>

    <script>


      <% if (flash.success && flash.success.length > 0) { %>


          Toastify({
              text: "<%= flash.success %>",
              duration: 2000,
              destination: "",
              newWindow: true,
              close: true,
              avatar: "https://cdn-icons-png.flaticon.com/128/7518/7518748.png",

              gravity: "top", // `top` or `bottom`
              position: "center", // `left`, `center` or `right`
              stopOnFocus: true, // Prevents dismissing of toast on hover
              style: {
                  background: "#000000",
                  borderRadius: "10px",
              },
              onClick: function(){} // Callback after click
          }).showToast();
      <% } %>

      <% if (flash.error && flash.error.length > 0) { %>
          Toastify({
              text: "<%= flash.error %>",
              duration: 2000,
              destination: "",
              newWindow: true,
              close: true,
              avatar: "https://cdn-icons-png.flaticon.com/128/9426/9426995.png",
              gravity: "top", // `top` or `bottom`
              position: "center", // `left`, `center` or `right`
              stopOnFocus: true, // Prevents dismissing of toast on hover
              style: {
                  background: "#000000",
                  borderRadius: "10px",
                  color: "white",
              },
              onClick: function(){} // Callback after click
          }).showToast();
      <% } %>
    </script>
    <script src="
    https://cdn.jsdelivr.net/npm/lodash@4.17.21/lodash.min.js
    "></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.5.4/socket.io.js"></script>

    <script type="module">
      <% if (locals.user){ %>
        import CallEngine from "<%= assetPath('js/ChatEngine.js') %>";

          new CallEngine(
            "user-chat-box",
            "<%= locals.user._id %>",
            "<%= locals.user.email %>",
            "<%= locals.user.name %>",
            "<%= locals.user.avatar %>",
            "https://sanam.social"
          );
      <% } %>
    </script>

